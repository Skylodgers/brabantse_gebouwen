{
  "name": "brabantse_gebouwen",
  "graph": "Building",
  "mappings": {
    "single": {
      "dataset": "monuments.json",
      "card_order": "0",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },        
        {
          "from": "id",
          "to": "identifier_content",
          "constants": [
            {
              "identifier_type": "Brabant Cloud ARK Identifier"

            }
          ]
        },
        {
          "from": "name",
          "to": "name_content",
          "constants": [
            {
              "name_content_type": "Primary",
              "name_content_language": "Nederlands",
              "language_type": "Nederlands"
            }
          ]
        },
        {
          "from": "monumentStatus",
          "to": "building_status"
        },
        {
          "from": "rightsURI ",
          "to": "rights_holder",
          "constants": [
            {
              "rights_type": "CCO"
            }
          ]
        },
        {
          "from": "dateDestroyed",
          "to": "destruction_timespan_start"
        },
        {
          "from": "dateInUse",
          "to": "in_use_timespan_start"
        },
        {
          "from": "dateNoLongerInUse",
          "to": "in_use_timespan_end"
        },
        {
          "from": "replacesPreviousURI",
          "to": "previous_building",
          "function": {
            "name": "related_resource"
          }
        },
        {
          "from": "isReplacedByURI",
          "to": "has_part",
          "function": {
            "name": "related_resource"
          }
        }
        
      ]
    },
    "other_identifiers": {
      "dataset": "monuments.json",
      "card_order": "1",
      "node_mappings": [

        {
          "from": "wikibaseURI",
          "to": "other_identifier_content",
          "constants": [
            {
              "other_identifier_type": "Wikibase"
            }
          ]
        },
        {
          "from": "regioWiki",
          "to": "other_identifier_content",
          "constants": [
            {
              "other_identifier_type": "regioWiki"
            }
          ]
        },
        {
          "from": "monumentLinkRCE",
          "to": "other_identifier_content",
          "constants": [
            {
              "other_identifier_type": "RCE Monuments"

            }
          ]

        }
      ]
    
    },

      "production_date": {
      "dataset": "monuments.json",
      "card_order": "13",
      "node_mappings": [

        {
          "from": "dateCreated",
          "to": "production_event_timespan_start"
        }        

      ]
    
    },

    "building_type": {
      "dataset": "monuments.json",
      "card_order": "1",
      "node_mappings": [

        {
          "from": "typeOfBuilding",
          "to": "building_type"
        }


      ]
    },
    "religious_order": {
      "dataset": "monuments.json",
      "card_order": "2",
      "node_mappings": [

        {
          "from": "religiousOrder",
          "to": "rel_order_tmp"
        }
      ]
    },
    "alternate_names": {
      "dataset": "monuments.json",
      "card_order": "3",
      "node_mappings": [

        {
          "from": "alternateName",
          "to": "alternative_name_content",
          "constants": [
            {
              "alternative_name_content_type": "Alternative",
              "alternative_name_content_language": "Nederlands"

            }
          ]

        }
      ]
    },
    "religion": {
      "dataset": "monuments.json",
      "card_order": "4",
      "node_mappings": [

        {
          "from": "religionURI",
          "to": "consecration_activity_religion"
        }
      ]
    },
    "places": {
      "dataset": "monuments.json",
      "card_order": "5",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },        
        {
          "from": "placeURI",
          "to": "place_dict"
        }
      ]
    },
    "actors": {
      "dataset": "monuments.json",
      "card_order": "6",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },        
        {
          "from": "architectURI",
          "to": "architect_dict"
        },
        {
          "from": "dedicationURI",
          "to": "dedication_dict"
        }
      ]
    },
    "dedications": {
      "dataset": "monuments.json",
      "card_order": "7",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },        
        {
          "from": "dedicationURI",
          "to": "dedication_dict"
        }
      ]
    },
    "architects": {
      "dataset": "monuments.json",
      "card_order": "8",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },
        {
          "from": "architectURI",
          "to": "architect_dict"
        }
      ]
    },
    "organisation": {
      "dataset": "monuments.json",
      "card_order": "9",
      "node_mappings": [
        {
          "from": "id",
          "to": "resource_id"
        },        
        {
          "from": "architectURI",
          "to": "architect_dict"
        }
      ]
    },
    "building_aat_type": {
      "dataset": "monuments.json",
      "card_order": "10",
      "node_mappings": [

        {
          "from": "additionalType",
          "to": "building_aat_type",
          "function": {
            "name": "lookup",
            "args": {
              "lookup_file": "building_aat_type.csv"
            }
          }        
        }
      ]
    },
    "images": {
      "dataset": "monuments.json",
      "card_order": "11",
      "node_mappings": [

        {
          "from": "imageURI",
          "to": "image_uri"
        }
      ]
    }
    
  }
}




